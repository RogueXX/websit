                                    <div class="wrapper">
                                        <a href="/home/index.html" class="yq-logo">
                                            <img src="/static/images/logo_01.png">
                                        </a>
                                        <div class="yq-channel">
                                            <ul class="channel-list">
                                                
                                                <li class="channel-item selected">
                                                    <a data-app-ignore="true" href="/home/index" class="channel-link" style="color: #fff;">首页</a>
                                                </li>
                                                <li class="channel-item">
                                                    <a data-app-ignore="true" href="/site/meeting" class="channel-link" style="color: #fff;">议程</a>
