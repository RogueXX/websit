                                                </li>
                                                <li>
                                                    <a href="contact-us.html" target="_blank" class="page pc">联系我们</a>
                                                </li>
                                            </ul>
                                            <div class="btns">

                                                <a href="#" target="_self" style="display: none;">ENGLISH</a>
                                                <a href="#" target="_self">我的票务</a>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div> 

                        </div> 

                    </div>

                </div>
                <!--col -->

            </div>
            <!-- row -->
        </div>
        <!-- container -->

    </div>

    @RenderBody()

    @*<footer>
        <p class="text-center">
            <small>Copyright &copy; @DateTime.Now.Year - <a href="#" target="_blank">MS2017</a> All Rights Reserved</small>
        </p>
    </footer>*@
    <script>
        if (!isMobile) {
            window.onresize = function () {
                if (window.innerWidth < 768) {
                    window.isMobile = true;
                } else {
                    window.isMobile = false;
                }
            }
        }
    </script>

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
